        </div>
        <div class="row">
            <div class="col-md-4">
                @Html.Label("ddl_idfreteporconta", "Frete por Conta", new { @class = "control-label required" })
                @Html.DropDownList("ddl_idfreteporconta", new SelectList(ViewBag.FretePorConta, "Id", "Nome"), new { @class = "form-control", tabindex = 7 })
            </div>
            <div class="col-md-4">
                @Html.Label("txt_valorfrete", "Valor Frete", new { @class = "control-label required" })
                @Html.TextBox("txt_valorfrete", null, new { @class = "form-control", tabindex = 8 })
            </div>
            <div class="col-md-4">
                @Html.Label("txt_valordesconto", "Valor Desconto", new { @class = "control-label required" })
                @Html.TextBox("txt_valordesconto", null, new { @class = "form-control", tabindex = 9 })
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                @Html.Label("txt_valoricms", "Valor ICMS", new { @class = "control-label required" })
                @Html.TextBox("txt_valoricms", null, new { @class = "form-control", tabindex = 10 })
            </div>
            <div class="col-md-4">
                @Html.Label("txt_valoripi", "Valor IPI", new { @class = "control-label required" })
                @Html.TextBox("txt_valoripi", null, new { @class = "form-control", tabindex = 11 })
            </div>
            <div class="col-md-4">
                @Html.Label("txt_valortotalnota", "Valor Total Nota", new { @class = "control-label required" })
                @Html.TextBox("txt_valortotalnota", null, new { @class = "form-control", tabindex = 12 })
            </div>
        </div>
        <div class="row">
            <div class="col-md-4 invisivel">
                @Html.Label("txt_dataentrada", "Data Entrada", new { @class = "control-label required" })
                @Html.TextBox("txt_dataentrada", null, new { @class = "form-control" })
            </div>
            <div class="col-md-2 invisivel">
                <div class="form-group" style="margin-top:20px">
                    @Html.Label("cbx_ehcancelada", "Cancelada", new { @class = "control-label required" })
                    @Html.CheckBox("cbx_ehcancelada")
