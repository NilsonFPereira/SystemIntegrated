@model List<SystemIntegrated.Models.Operacao.EntradaNotaViewModel>
@{
    var lista = Model;
    ViewBag.Title = "Cadastro de Entradas";
    var titulo = ViewBag.Title;
}
<link href="~/Content/Operacao/OperEntradaNota.css" rel="stylesheet" type="text/css" />
<link href="~/Content/Jquery-ui/jquery-ui.css" rel="stylesheet" />
<link href="~/Content/Jquery-ui/jquery-ui.theme.min.css" rel="stylesheet" />
<style>
    .modal-dialog {
        width: 1000px !important;

    }
</style>
<div class="col-md-12" style="margin: 0 auto">
    <section id="cadastro_cabecalho">
        <h2 style="color:#000">@ViewBag.Title</h2>
    </section>
    <div id="cadastro_conteudo">
        <div id="cabecalho_grid">
            <div class="form-group">
                <div class="col-md-2">
                    <a id="btn_incluir" class="btn btn-success">
                        <i class="glyphicon glyphicon-plus"></i> Incluir
                    </a>
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-4">
                    @Html.DropDownList("ddl_tam_pag",
                                        (SelectList)ViewBag.ListaTamPag,
                                        new { @class = "selectpicker form-control" }
                                      )
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-6">
                    <div class="form-group left-inner-addon">
                        <i class="glyphicon glyphicon-search"></i>
                        @Html.TextBox("txt_filtro",
                                       null,
                                       new { @class = "form-control" }
                                     )
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-12" style="overflow:auto">
            <table id="grid_cadastro"
                   class="table
                          table-bordered
                          table-striped
                          table-hover
                          @Html.Raw(lista == null || lista.Count == 0 ? "invisivel" : "")">
                <thead>
                    <tr class="info">
                        <th>#</th>
                        <th>Dt. Emissão</th>
                        <th>Nº Nota</th>
                        <th>Chave Acesso</th>
                        <th>Total Produto</th>
                        <th>Total Nota</th>
                        <th>Desconto</th>
                        <th>Frete</th>
                        <th>ICMS</th>
                        <th>IPI</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        foreach (var item in Model)
                        {
                            <tr data-id="@item.Id">
                                <td>
                                    <a id="detalhamento" class="btn btn-primary">
                                        <i class="fa fa-bars"></i>
                                    </a>
                                </td>
                                <td>@item.DataEmissao</td>
                                <td class="text-right">@item.NumeroNota</td>
                                <td>@item.ChaveAcesso</td>
                                <td class="text-right">@item.ValorTotalProdutos</td>
                                <td class="text-right">@item.ValorTotalNota</td>
                                <td class="text-right">@item.ValorDesconto</td>
                                <td class="text-right">@item.ValorFrete</td>
                                <td class="text-right">@item.ValorIcms</td>
                                <td class="text-right">@item.ValorIpi</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
            <div id="mensagem_grid" @Html.Raw(lista != null && lista.Count > 0 ? "class=\"invisivel\"" : "")>
                Nenhum registro em @titulo
            </div>
            @if (ViewBag.QuantPaginas > 1)
            {
                <div class="row">
                    <nav>
                        <ul class="pagination">
                            <li class="page-item active">
                                <a class="page-link">1</a>
                            </li>
                            @for (var i = 2; i < ViewBag.QuantPaginas; i++)
                            {
                                <li class="page-item">
                                    <a class="page-link">@i</a>
                                </li>
                            }
                        </ul>
                    </nav>
                </div>
            }
        </div>
    </div>
</div>
@Html.Hidden("id_cadastro")

<!--modal cadastro -->
<div id="modal_cadastro" class="form-horizontal invisivel" role="dialog">
    <div id="msg_mensagem_aviso" class="alert alert-danger invisivel"></div>
    <fieldset>
        <div class="row">
            <div class="col-md-12">
                <h3 class="page-header" style="color:#000">Dados da Nota</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                @Html.Label("txt_dataemissao", "Data Emissão", new { @class = "control-label required" })
                @Html.TextBox("txt_dataemissao", null, new { @class = "form-control", tabindex = 1 })
            </div>
            <div class="col-md-3">
                @Html.Label("txt_numeronota", "Número Nota", new { @class = "control-label required" })
                @Html.TextBox("txt_numeronota", null, new { @class = "form-control", tabindex = 2 })
            </div>
            <div class="col-md-6">
                @Html.Label("ddl_idnatureza", "Natureza", new { @class = "control-label required" })
                @Html.DropDownList("ddl_idnatureza", new SelectList(ViewBag.Natureza, "Id", "Nome"), new { @class = "form-control", tabindex = 3 })
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                @Html.Label("txt_chaveacesso", "Chave de Acesso", new { @class = "control-label required" })
                @Html.TextBox("txt_chaveacesso", null, new { @class = "form-control", tabindex = 4 })
            </div>
            <div class="col-md-4">
                @Html.Label("txt_valortotalprodutos", "Valor Total Produtos", new { @class = "control-label required" })
                @Html.TextBox("txt_valortotalprodutos", null, new { @class = "form-control", tabindex = 5 })
            </div>
        </div>
        <div class="row">
            <div class="col-md-2">
                @Html.Label("txt_idfornecedor", "Código", new { @class = "control-label required" })
                @Html.TextBox("txt_idfornecedor", null, new { @class = "form-control" })
            </div>
            <div class="col-md-10">
                @Html.Label("suggest_fornecedor", "Fornecedor", new { @class = "control-label required" })
                @Html.TextBox("suggest_fornecedor", null, new { @class = "form-control", type = "text", tabindex = 6 })
