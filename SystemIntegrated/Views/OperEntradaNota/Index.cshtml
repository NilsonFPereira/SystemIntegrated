        function desabilitar_campos_nota() {

            $('#txt_dataemissao').attr('disabled', true);
            $('#txt_numeronota').attr('disabled', true);
            $('#ddl_idnatureza').attr('disabled', true);
            $('#txt_chaveacesso').attr('disabled', true);
            $('#txt_valortotalprodutos').attr('disabled', true);
            $('#suggest_fornecedor').attr('disabled', true);
            $('#ddl_idfreteporconta').attr('disabled', true);
            $('#txt_valorfrete').attr('disabled', true);
            $('#txt_valordesconto').attr('disabled', true);
            $('#txt_valoricms').attr('disabled', true);
            $('#txt_valoripi').attr('disabled', true);
            $('#txt_valortotalnota').attr('disabled', true);

        }

        function formatar_mensagens_aviso(mensagens) {

            var ret = '';

            for (var i = 0; i < mensagens.length; i++) {

                ret += '<li>' + mensagens[i] + '</li>';
                break;

            }
            return '<ul>' + ret + '</ul>';
        }

        function criar_linha_grid_itens(dados) {

            var ret =
                '<tr data_idproduto=' + dados.Id + '>' +
                '<td>' + dados.IdProduto + '</td>' +
                '<td>' + dados.NomeProduto + '</td>' +
                '<td>' + dados.UnidadeMedida + '</td>' +
                '<td>' + dados.QuantidadeProduto + '</td>' +
                '<td>' + dados.ValorUnitarioProduto + '</td>' +
                '<td>' + dados.ValorTotalProduto + '</td>' +
                '</tr>';

            return ret;

        }

