                    select: function (event, ui) {

                        $('#suggest_produto').val(ui.item.Nome)
                        $('#txt_idproduto').val(ui.item.Id);
                        $('#txt_unidademedida').val(ui.item.Sigla);
                    }
                });
            })

            .on('blur', '#txt_quantidadeproduto', function () {

                var quantidadeproduto = $('#txt_quantidadeproduto').val(),
                    valorunitarioproduto = $('#txt_valorunitarioproduto').val(),
                    vl_unitarioproduto = parseFloat(valorunitarioproduto.replace(",", "."))


                if (quantidadeproduto > 0 && quantidadeproduto != "" && valorunitarioproduto != "") {

                    var somaValor = '';

                    somaValor = (quantidadeproduto * vl_unitarioproduto).toFixed(2);

                    $('#txt_valortotalproduto').val(somaValor.replace(".", ","));

                }
                else {
                    $('#txt_valortotalproduto').val('0,00');

                }
            })

            .on('blur', '#txt_valorunitarioproduto', function () {

                var quantidadeproduto = $('#txt_quantidadeproduto').val(),
                    valorunitarioproduto = $('#txt_valorunitarioproduto').val(),
                    vl_unitarioproduto = parseFloat(valorunitarioproduto.replace(",", "."))


                if (quantidadeproduto > 0 && quantidadeproduto != "" && valorunitarioproduto != "") {

                    var somaValor = '';

                    somaValor = (quantidadeproduto * vl_unitarioproduto).toFixed(2);

                    $('#txt_valortotalproduto').val(somaValor.replace(".", ","));

                }
                else {
                    $('#txt_valortotalproduto').val('0,00');

                }
            })

            .on('click', '#btn_incluirproduto', function () {

                var id = 1;

                var quant = $('#grid_itens >tbody > tr').length;

                var quant = quant + id;

                var btn = $(this),
                    url = '@Url.Action("InsertItemSession", "OperEntradaNotaItem")',
                    param = {
                        Id: quant,
                        IdProduto: $('#txt_idproduto').val(),
                        NomeProduto: $('#suggest_produto').val(),
                        UnidadeMedida: $('#txt_unidademedida').val(),
                        QuantidadeProduto: $('#txt_quantidadeproduto').val(),
                        ValorUnitarioProduto: formatar_decimal($('#txt_valorunitarioproduto').val()),
                        ValorTotalProduto: formatar_decimal($('#txt_valortotalproduto').val())
                    }

                $.post(url, param, function (response) {

                    if (response.Resultado == "OK") {


                        param.ValorUnitarioProduto = $('#txt_valorunitarioproduto').val();
                        param.ValorTotalProduto = $('#txt_valortotalproduto').val();

                        var table = $('#grid_itens').find('tbody');

                        table.append(criar_linha_itens(param));

                        $('#grid_itens').removeClass('invisivel');

                        limpar_campos_produtos();

                    }
                    else if (response.Resultado == "ERRO") {

                        $('#msg_erro').show();
                        $('#msg_mensagem_aviso_produto').val('');
                        $('#msg_mensagem_aviso_produto').hide();
                        $('#msg_aviso').hide();

                    } else if (response.Resultado == "AVISO") {

                        $('#msg_mensagem_aviso_produto').html(formatar_mensagens_aviso(response.Mensagens));
                        $('#msg_mensagem_aviso_produto').show();

                        setInterval(function () {
                            $('#msg_mensagem_aviso_produto').val('');
                            $('#msg_mensagem_aviso_produto').hide();
                        }, 5000);
                    }
                });
            })

            .on('click', '#btn_excluir_item', function () {
                var btn = $(this),
                    tr = btn.closest('tr'),
                    id = tr.attr('data_idproduto'),
                    url = '@Url.Action("DeleteItemSession", "OperEntradaNotaItem")',
                    param = { 'id': id };

                bootbox.confirm({
                    message: 'Deseja realmente excluir?',
                    buttons: {
                        confirm: {

                            label: 'SIM',
                            className: 'btn-danger'

                        }, cancel: {

                            label: 'NÃO',
                            className: 'btn-success'

                        }

                    },
                    callback: (function (result) {

                        if (result) {

                            $.post(url, param, function (response) {

                                if (response.Resultado == "OK") {

                                    tr.remove();

                                }
                            })
                        }
                    })
                })
            })

            .on('click', '#btn_confirmar', function () {

                var dataEmissao = '';

                if ($('#txt_dataemissao').val() != "") {

                    dataEmissao = formatar_data($('#txt_dataemissao').val());
                }

                var quant = $('#grid_itens >tbody > tr').length;

                if (quant == 0) {

                    $('#msg_mensagem_aviso').html('Nenhum produto foi informado');
                    $('#msg_aviso').show();
                    $('#msg_mensagem_aviso').show();

                    setInterval(function () {

                        $('#msg_mensagem_aviso').val('');
                        $('#msg_mensagem_aviso').hide();
                        $('#msg_aviso').hide();


                    }, 5000);

                }
                else {

                    var btn = $(this),
                        url = '@Url.Action("SalvarEntradaNota", "OperEntradaNota")',
                        param = {

                            Id: $('#id_cadastro').val(),
                            DataEmissao: dataEmissao,
                            NumeroNota: $('#txt_numeronota').val(),
                            ChaveAcesso: $('#txt_chaveacesso').val(),
                            IdNatureza: $('#ddl_idnatureza').val(),
                            ValorTotalProdutos: formatar_decimal($('#txt_valortotalprodutos').val()),
                            IdFornecedor: $('#txt_idfornecedor').val(),
                            IdFretePorConta: $('#ddl_idfreteporconta').val(),
                            ValorFrete: formatar_decimal($('#txt_valorfrete').val()),
                            ValorDesconto: formatar_decimal($('#txt_valordesconto').val()),
                            ValorIcms: formatar_decimal($('#txt_valoricms').val()),
                            ValorIpi: formatar_decimal($('#txt_valoripi').val()),
                            ValorTotalNota: formatar_decimal($('#txt_valortotalnota').val()),
                            DataEntrada: formatar_data($('#txt_dataentrada').val()),
                            EhCancelada: $('#cbx_ehcancelada').prop('checked')
                        };

                    $.post(url, param, function (response) {

                        if (response.Resultado == "OK") {

                            var loading = bootbox.dialog({
                                message: '<div class="text-center" style="color:#0000FF; font-size:14px"><i class= "fa fa-spinner fa-spin fa-2x fa-fw" ></i> Aguarde, inserindo os dados da nota...</div>',
                                closeButton: false

                            });

                            if (param.Id == 0) {

                                param.Id = response.IdSalvo;
                                param.DataEmissao = $('#txt_dataemissao').val();

                                var url = '@Url.Action("InserirItem", "OperEntradaNotaItem")',
                                    paramItens = { 'idEntradaNota': param.Id };

                                $.post(url, paramItens, function (responseItem) {


                                    if (responseItem.Resultado == "OK") {


                                        var table = $('#grid_cadastro').find('tbody'),
                                            linha = '';

                                        setTimeout(function () {
                                            loading.modal('hide'),

                                            $('#grid_cadastro').removeClass('invisivel');
                                            $('#mensagem_grid').addClass('invisivel');
                                            linha = criar_linha_grid(param);
                                            table.append(linha)

                                        }, 3000);

                                        $('#modal_cadastro').parents('.bootbox').modal('hide');

                                    } else if (responseItem.Resultado == "AVISO") {

                                        $('#msg_mensagem_aviso').html(formatar_mensagens_aviso(response.Mensagens));
                                        $('#msg_mensagem_aviso').show();
                                        $('#msg_aviso').show();
                                        $('#msg_erro').hide();

                                    }
                                });
                            }


                        } else if (response.Resultado == "ERRO") {

                            $('#msg_erro').show();
                            $('#msg_mensagem_aviso').val('');
                            $('#msg_mensagem_aviso').hide();
                            $('#msg_aviso').hide();

                        } else if (response.Resultado == "AVISO") {

                            $('#msg_mensagem_aviso').html(formatar_mensagens_aviso(response.Mensagens));
                            $('#msg_mensagem_aviso').show();
                            $('#msg_aviso').show();
                            $('#msg_erro').hide();

                            setInterval(function () {

                                $('#msg_mensagem_aviso').val('');
                                $('#msg_mensagem_aviso').hide();
                                $('#msg_aviso').hide();


                            }, 5000);
                        }
                    })

                }
            })

            .on('click', '#detalhamento', function () {

                var btn = $(this),
                    id = btn.closest('tr').attr('data-id'),
                    url = '@Url.Action("RecuperarEntradaNota", "OperEntradaNota")',
                    urlItens = '@Url.Action("RecuperarEntradaNotaItem", "OperEntradaNotaItem")',
                    param = { 'id': id };

                $.post(url, param, function (response) {

                    var loading = bootbox.dialog({
                        message: '<div class="text-center" style="color:#0000FF; font-size:14px"><i class= "fa fa-spinner fa-spin fa-2x fa-fw" ></i> Aguarde, as informações estão sendo carregadas...</div>',
                        closeButton: false

                    });

                    if (response) {

                        setTimeout(function () { loading.modal('hide'), abrir_detalhamento(response) }, 3000);

                    }


                })

                $.post(urlItens, param, function (responseItem) {


                    if (responseItem) {

                        if (responseItem.length > 0) {

                            var table = $('#grid_itens_nota').find('tbody');

                            table.empty()

                            for (var i = 0; i < responseItem.length; i++) {

                                table.append(criar_linha_grid_itens(responseItem[i]));

                            }
                        }
                    }
                });
            })

